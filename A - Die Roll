#include <iostream>
#include <string>
#include <time.h>
#include <algorithm>
using namespace std;

int main()
{
    int a , b;
    cin>> a >>b;
    int m = max(a, b);
    int p = 6 - m +1 ;
    
     if ( p == 6)
        cout<< 1 << "/" <<1;
    else if (p%2 == 0)
        cout<< p / 2 <<"/"<<3 ;
    else if (p ==3 )
        cout << 1 << "/" << 2;
    else 
        cout<< p << "/" <<6;
        

}
