#include <iostream>
#include <cmath>
 
using namespace std;
 
int main()  
{
    int moves {0};
    int x ;
    for (int j {0}; j<5; j++)
        for (int i {0}; i<5; i++)
            {
                cin>>x;
                if (x == 1) {
                    moves+= abs(j-2);
                    moves+= abs(i-2);
                    break;
                    }
            }
    cout<<moves<<endl;
    
} 
